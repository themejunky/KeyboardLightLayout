<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:kplus="http://schemas.android.com/apk/res-auto">

    <!-- checked -->
    <item android:state_checked="true" kplus:key_type_feedback="true" android:drawable="@drawable/sym_keyboard_shift_locked" />
    <item android:state_checked="true" android:drawable="@drawable/sym_keyboard_shift_locked" />

    <!-- pressed -->
    <item android:state_pressed="true" kplus:key_type_feedback="true" android:drawable="@drawable/sym_keyboard_shift_locked" />
    <item android:state_pressed="true" android:drawable="@drawable/sym_keyboard_shift_locked" />
    
    <!-- Normal keys -->
	<item kplus:key_type_feedback="true" android:drawable="@drawable/sym_keyboard_shift" />
    <item android:drawable="@drawable/sym_keyboard_shift" />
</selector>
